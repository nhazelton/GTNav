<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GTNav.MenuPage">
    <ContentPage.Content>
        <StackLayout BackgroundColor="Gray">
            <Button HeightRequest="100" Text="Main Map" TextColor="#262626" BackgroundColor="#eeb211" Command="{Binding GoHomeCommand}"/>
            <Button HeightRequest="100" Text="Bus Routes" TextColor="#e5e5e5" BackgroundColor="#00254c" Command="{Binding GoSecondCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>